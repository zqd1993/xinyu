<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.mshy.VInterestSpeed.common.ui.view.ShapeLinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingLeft="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="12dp"
        android:layout_marginRight="15dp"
        android:layout_marginBottom="8dp"
        android:paddingTop="21dp"
        android:paddingRight="16dp"
        android:paddingBottom="19dp"
        app:layout_constraintTop_toTopOf="parent"
        app:sll_layoutRadius="12dp"
        app:sll_solidColor="@color/color_F5F5F5">

        <TextView
            android:id="@+id/tv_bill_vqu_item_withdrawal_list_get_money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/color_textMain"
            android:textSize="13sp"
            tools:text="提现金额：100元" />

        <TextView
            android:id="@+id/tv_bill_vqu_item_withdrawal_list_real_money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:textColor="@color/color_textMain"
            android:textSize="13sp"
            tools:text="提现金额：100元" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp">

            <TextView
                android:id="@+id/tv_bill_vqu_item_withdrawal_list_account"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_textMain"
                android:textSize="13sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:text="提现金额：100元" />

            <TextView
                android:id="@+id/tv_bill_vqu_item_withdrawal_status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_textMain"
                android:textSize="13sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:text="提现金额：100元" />
        </androidx.constraintlayout.widget.ConstraintLayout>


        <TextView
            android:id="@+id/tv_bill_vqu_item_withdrawal_create_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:textColor="@color/color_textMain"
            android:textSize="13sp"
            tools:text="提现金额：100元" />

        <TextView
            android:id="@+id/tv_bill_vqu_item_withdrawal_order_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:textColor="@color/color_textMain"
            android:textSize="13sp"
            tools:text="提现金额：100元" />
    </com.mshy.VInterestSpeed.common.ui.view.ShapeLinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>