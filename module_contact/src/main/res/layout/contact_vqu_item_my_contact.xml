<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cl_contact_vqu_item_my_contact_parent"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="15dp"
    android:layout_marginRight="15dp"
    android:layout_marginTop="@dimen/dp_size_8"
    android:elevation="0dp"
    app:cardBackgroundColor="@color/color_FFFFFF"
    app:cardCornerRadius="10dp"
    app:cardElevation="1dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintTop_toTopOf="parent">

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingBottom="@dimen/dp_size_20">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/iv_contact_vqu_item_my_contact_avatar"
                android:layout_width="42dp"
                android:layout_height="42dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="15dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_contact_vqu_item_my_contact_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="15dp"
                android:textColor="#383838"
                android:textSize="15sp"
                android:textStyle="bold"
                app:layout_constraintLeft_toRightOf="@id/iv_contact_vqu_item_my_contact_avatar"
                app:layout_constraintTop_toTopOf="parent"
                tools:text="🪐娜宝BABY⭐️" />


            <include
                android:id="@+id/sll_gender"
                layout="@layout/common_vqu_include_gender"
                android:layout_width="wrap_content"
                android:layout_height="15dp"
                android:layout_marginLeft="2dp"
                app:layout_constraintBottom_toBottomOf="@id/tv_contact_vqu_item_my_contact_name"
                app:layout_constraintLeft_toRightOf="@id/tv_contact_vqu_item_my_contact_name"
                app:layout_constraintTop_toTopOf="@id/tv_contact_vqu_item_my_contact_name" />

            <ImageView
                android:id="@+id/iv_contact_vqu_item_my_contact_auth"
                android:layout_width="37dp"
                android:layout_height="15dp"
                android:layout_marginLeft="2dp"
                android:src="@mipmap/ic_contact_vqu_real_man"
                app:layout_constraintBottom_toBottomOf="@id/sll_gender"
                app:layout_constraintLeft_toRightOf="@id/sll_gender"
                app:layout_constraintTop_toTopOf="@id/sll_gender" />

            <TextView
                android:id="@+id/tv_contact_vqu_item_my_contact_id"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="6dp"
                android:drawableRight="@mipmap/ic_mine_copy"
                android:drawablePadding="2dp"
                android:includeFontPadding="false"
                android:textColor="@color/color_999999"
                android:textSize="12sp"
                app:layout_constraintLeft_toRightOf="@id/iv_contact_vqu_item_my_contact_avatar"
                app:layout_constraintTop_toBottomOf="@id/tv_contact_vqu_item_my_contact_name"
                tools:text="ID:000000" />

            <TextView
                android:id="@+id/tv_contact_vqu_item_my_contact_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="6dp"
                android:includeFontPadding="false"
                android:textColor="@color/color_999999"
                android:textSize="12sp"
                app:layout_constraintLeft_toRightOf="@id/iv_contact_vqu_item_my_contact_avatar"
                app:layout_constraintTop_toBottomOf="@id/tv_contact_vqu_item_my_contact_id"
                tools:text="注册时间：2021-11-22" />


            <TextView
                android:id="@+id/tv_contact_vqu_item_my_contact_money"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="15dp"
                android:includeFontPadding="false"
                android:textColor="@color/color_B4A3FD"
                android:textSize="15sp"
                app:layout_constraintBottom_toBottomOf="@id/tv_contact_vqu_item_my_contact_yuan"
                app:layout_constraintRight_toLeftOf="@id/tv_contact_vqu_item_my_contact_yuan"
                app:layout_constraintTop_toTopOf="@id/tv_contact_vqu_item_my_contact_yuan"
                tools:text="128.12" />

            <TextView
                android:id="@+id/tv_contact_vqu_item_my_contact_yuan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:layout_marginRight="15dp"
                android:text="元"
                android:textColor="#383838"
                android:textSize="12sp"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tv_contact_vqu_item_My_contact_desc"

                />

            <TextView
                android:id="@+id/tv_contact_vqu_item_My_contact_desc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="15dp"
                android:includeFontPadding="false"
                android:text="@string/contact_vqu_give_unit"
                android:textColor="#383838"
                android:textSize="12sp"
                app:layout_constraintBottom_toBottomOf="@id/tv_contact_vqu_item_my_contact_name"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@id/tv_contact_vqu_item_my_contact_name" />
        </androidx.constraintlayout.widget.ConstraintLayout>


        <LinearLayout
            android:id="@+id/ll_contact_vqu_my_contact_more_parent_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"
            android:orientation="horizontal"
            android:visibility="gone"
            tools:visibility="visible">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_contact_vqu_item_my_contact_letter_num"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="0"
                    android:textStyle="bold"
                    android:textColor="@color/color_textMain"
                    android:textSize="14sp" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:gravity="center"
                    android:text="文字收益"
                    android:textColor="@color/color_textDesc"
                    android:textSize="12sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_contact_vqu_item_my_contact_video_num"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/color_textMain"
                    android:textSize="14sp"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:gravity="center"
                    android:textColor="@color/color_textDesc"
                    android:text="视频收益"
                    android:textSize="12sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_contact_vqu_item_my_contact_voice_num"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/color_textMain"
                    android:textSize="14sp"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:gravity="center"
                    android:text="语音收益"
                    android:textColor="@color/color_textDesc"
                    android:textSize="12sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_contact_vqu_item_my_contact_gift_num"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/color_textMain"
                    android:textSize="14sp"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:gravity="center"
                    android:textColor="@color/color_textDesc"
                    android:text="礼物收益"
                    android:textSize="12sp" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_contact_vqu_my_contact_more_parent_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="21dp"
            android:orientation="horizontal"
            android:visibility="gone"
            tools:visibility="visible">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"

                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_contact_vqu_item_my_contact_online_time"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/color_textMain"
                    android:textSize="14sp"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:gravity="center"
                    android:textColor="@color/color_textDesc"
                    android:text="在线时长"
                    android:textSize="12sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_contact_vqu_item_my_contact_heart_num"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/color_textMain"
                    android:textSize="14sp"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:gravity="center"
                    android:text="心动次数"
                    android:textColor="@color/color_textDesc"
                    android:textSize="12sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_contact_vqu_item_my_contact_chat_num"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/color_textMain"
                    android:textSize="14sp"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:gravity="center"
                    android:textColor="@color/color_textDesc"
                    android:text="会话场数"
                    android:textSize="12sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_contact_vqu_item_my_contact_recharge_num"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/color_textMain"
                    android:textSize="14sp"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:gravity="center"
                    android:textColor="@color/color_textDesc"
                    android:text="充值"
                    android:textSize="12sp" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_contact_vqu_my_contact_more_parent_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="21dp"
            android:orientation="horizontal"
            android:visibility="gone"
            tools:visibility="visible">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv_contact_vqu_item_my_contact_last_time"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/color_textMain"
                    android:textSize="14sp"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:gravity="center"
                    android:text="最后登录"
                    android:textColor="@color/color_textDesc"
                    android:textSize="12sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical"
                android:visibility="invisible">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/color_273145"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    tools:text="938" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:gravity="center"
                    android:textSize="12sp"
                    tools:text="文字收益" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical"
                android:visibility="invisible">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/color_273145"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    tools:text="938" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:gravity="center"
                    android:textSize="12sp"
                    tools:text="文字收益" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical"
                android:visibility="invisible">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/color_273145"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    tools:text="938" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:gravity="center"

                    android:textSize="12sp"
                    tools:text="文字收益" />
            </LinearLayout>

        </LinearLayout>

        <TextView
            android:id="@+id/tv_contact_vqu_item_my_contact_more"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="10dp"
            android:drawableRight="@drawable/selector_small_purple_arrow_up_and_down"
            android:drawablePadding="8dp"
            android:paddingLeft="20dp"
            android:paddingTop="10dp"
            android:paddingRight="20dp"
            android:text="展开"
            android:textColor="#335BB0"
            android:textSize="12sp"
            android:visibility="gone"
            tools:visibility="visible" />
    </androidx.appcompat.widget.LinearLayoutCompat>
</androidx.cardview.widget.CardView>