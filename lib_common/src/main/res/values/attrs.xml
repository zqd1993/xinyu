    <!--ShapeTextView-->
    <declare-styleable name="ShapeTextView">
        <!--是否使用Selector选择器-->
        <attr name="stv_openSelector" format="boolean" />
        <!--按下填充色-->
        <attr name="stv_solidTouchColor" format="color" />
        <!--按下边框色-->
        <attr name="stv_strokeTouchColor" format="color" />
        <!--填充色-->
        <attr name="stv_solidColor" format="color" />
        <!--边框色-->
        <attr name="stv_strokeColor" format="color" />
        <!--边框宽度-->
        <attr name="stv_strokeWidth" format="dimension" />
        <!--设置渐变色-->
        <attr name="stv_startColor" format="color" />
        <attr name="stv_endColor" format="color" />
        <!--圆角弧度-->
        <attr name="stv_layoutRadius" format="dimension" />
        <!--四个角的圆角弧度-->
        <attr name="stv_topLeftRadius" format="dimension" />
        <attr name="stv_topRightRadius" format="dimension" />
        <attr name="stv_bottomLeftRadius" format="dimension" />
        <attr name="stv_bottomRightRadius" format="dimension" />
        <!--虚线边框宽度-->
        <attr name="stv_dashWidth" format="dimension" />
        <!--虚线边框间隙-->
        <attr name="stv_dashGap" format="dimension" />
        <!-- Standard orientation constant. -->
        <attr name="stv_orientation">
            <!-- 渐变从左至右 -->
            <enum name="stv_horizontal" value="0" />
            <!-- 渐变从上至下 -->
            <enum name="stv_vertical" value="1" />
        </attr>

    </declare-styleable>

